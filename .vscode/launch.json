    "version": "0.2.0",
    "configurations": [
        {
            "name": "NPM Clean Start",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "clean-start"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        },

        {
            "type": "pwa-node",
            "request": "launch",
            "name": "NPM Launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeArgs": [
                "run",
                "start"
            ],
            "runtimeExecutable": "npm"
        }
    ]
}